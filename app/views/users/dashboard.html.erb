<div class="background-art">
</div>
<div class="container mt-4">
  <div class="row justify-content-center">
    <div class="col-10">
      <div class="row">
        <div class="col-6">
          <div class="dashboard-card">
            <h3 class="text-wallet">Wallet</h3>
            <div class="balance-circles">
              <div class="balance-circle">
                <h3 class="text-circle">Balance</h3>
                <h3>$<%= @dollar_balance %></h3>
              </div>
              <div class="balance-circle">
                <h3 class="text-circle">Earnings</h3>
                <h3 class="price-up">$<%= @earnings %></h3>
              </div>
            </div>
          </div>
        </div>
        <div class="col-6">
          <div class="dashboard-card">
            <h3 class="text-graph">Graph</h3>
            <img src="data:image/jpeg;base64,">
          </div>
        </div>
      </div>
      <div class="row mt-4">
        <div class="col-12">
          <div class="dashboard-card">
            <h3 class="text-balance">Asset Overview</h3>
            <div class="holdings-info">
              <div class="holding-item">
                <p>Asset</p>
                <p>BTC</p>
              </div>
              <div class="holding-item">
                <p>Balance</p>
                <p>0.004BTC</p>
              </div>

              <div class="holding-item">
                <p>Price</p>
                <p>50,000$</p>
              </div>
              <div class="holding-item">
                <p>24h change</p>
                <p class="price-up">+2.4%</p>
              </div>


            </div>

          </div>
        </div>
      </div>
    </div>
  </div>

</div>
